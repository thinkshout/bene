{
    "name": "drupal/bene",
    "description": "Bene distribution for non-profits",
    "type": "drupal-profile",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "authors": [
      {
          "name": "ThinkShout",
          "role": "Developer"
      }
    ],
    "repositories": [
      {
        "type": "composer",
        "url": "https://packages.drupal.org/8"
      }
    ],
  "prefer-stable": true,
  "require": {
      "composer/installers": "^1.2",
      "cweagans/composer-patches": "~1.5",
      "drupal/core": "~8.3.0",
      "drupal/admin_toolbar": "1.17",
      "drupal/config_update": "^1.1",
      "drupal/contact_storage": "1.0-beta2",
      "drupal/ctools": "3.0-alpha26",
      "drupal/default_content": "1.x-dev",
      "drupal/ds": "2.3",
      "drupal/email_registration": "1.0-rc3",
      "drupal/features": "^3.0",
      "drupal/google_analytics": "2.0",
      "drupal/inline_entity_form": "1.0-alpha6",
      "drupal/linkit": "4.1",
      "drupal/layout_plugin": "1.0-alpha22",
      "drupal/metatag": "1.0-beta9",
      "drupal/pathauto": "1.0-alpha3",
      "drupal/r4032login": "1.x-dev",
      "drupal/simple_sitemap": "2.5",
      "drupal/token": "1.0-alpha2"
    },
    "require-dev": {
      "drupal/devel": "1.x-dev",
      "behat/mink": "~1.7",
      "behat/mink-goutte-driver": "~1.2",
      "jcalderonzumba/gastonjs": "~1.0.2",
      "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
      "mikey179/vfsStream": "~1.2",
      "phpunit/phpunit": "~4.8",
      "symfony/css-selector": "~2.8"
    },
    "extra": {
      "patches": {
        "drupal/core": {
          "Telephone field with 5 digits or less fatally breaks formatter": "https://www.drupal.org/files/issues/2484693-47.patch"
        },
        "drupal/metatag": {
          "Drupal console error": "https://www.drupal.org/files/issues/drupal-consol_2760661.patch"
        }
      }
  }


}
